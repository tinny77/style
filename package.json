{
  "name": "@volverjs/style",
  "version": "1.0.0",
  "description": "@volverjs/style is a lightweight responsive CSS utility library to accompany @volverjs/ui and static webpages as well.",
  "main": "dist/style.css",
  "style": "dist/style.css",
  "type": "module",
  "engines": {
    "node": ">= 12.x"
  },
  "packageManager": "pnpm@7.8.0",
  "scripts": {
    "dev": "vite --port 3000 --open",
    "build": "vite build --base=./ && node ./scripts/build.js"
  },
  "files": [
    "src/*",
    "dist/*"
  ],
  "exports": {
    ".": "./dist/style",
    "./components": "./dist/components",
    "./components/vv-button-group": "./dist/components/vv-button-group",
    "./components/vv-button": "./dist/components/vv-button",
    "./components/vv-table": "./dist/components/vv-table",
    "./components/vv-text": "./dist/components/vv-text",
    "./preflight": "./dist/preflight",
    "./props/borders": "./dist/props/borders",
    "./props/colors": "./dist/props/colors",
    "./props": "./dist/props",
    "./props/layout": "./dist/props/layout",
    "./props/media": "./dist/props/media",
    "./props/sizing": "./dist/props/sizing",
    "./props/spacing": "./dist/props/spacing",
    "./props/typography": "./dist/props/typography",
    "./reset": "./dist/reset",
    "./themes/dark/components": "./dist/themes/dark/components",
    "./themes/dark/props": "./dist/themes/dark/props",
    "./themes/dark": "./dist/themes/dark/style",
    "./utilities/backgrounds": "./dist/utilities/backgrounds",
    "./utilities/borders": "./dist/utilities/borders",
    "./utilities/colors": "./dist/utilities/colors",
    "./utilities/effects": "./dist/utilities/effects",
    "./utilities/flexbox": "./dist/utilities/flexbox",
    "./utilities": "./dist/utilities",
    "./utilities/layout": "./dist/utilities/layout",
    "./utilities/sizing": "./dist/utilities/sizing",
    "./utilities/spacing": "./dist/utilities/spacing",
    "./utilities/typography": "./dist/utilities/typography"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/volverjs/style"
  },
  "keywords": [
    "css",
    "design-system",
    "volverjs"
  ],
  "author": "24/Consulting",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volverjs/style/issues"
  },
  "homepage": "https://github.com/volverjs/style#readme",
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "glob": "^8.0.3",
    "postcss": "^8.4.17",
    "postcss-preset-env": "^7.8.2",
    "promise": "^8.2.0",
    "sass": "^1.55.0",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^5.0.0",
    "vite": "^3.1.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-handlebars": "^1.6.0",
    "vite-plugin-stylelint": "^3.0.7"
  }
}